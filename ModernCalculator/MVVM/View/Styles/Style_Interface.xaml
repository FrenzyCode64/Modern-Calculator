<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ModernCalculator.MVVM.View.CustomControls">
    <LinearGradientBrush x:Key="gradient_skin_1"
                         StartPoint="0, 0" EndPoint="1, 1">
        <GradientStop Color="#B24592" Offset="0"/>
        <GradientStop Color="#F15F79" Offset="1"/>
    </LinearGradientBrush>

    <Style x:Key="style_Interface_skin_1" TargetType="{x:Type local:Button_Interface}">
        <Setter Property="Background" Value="{StaticResource gradient_skin_1}"/>
    </Style>
</ResourceDictionary>